
@{
    ViewData["Title"] = "About";
}

<h2>About</h2>
<div>"This is the about page"</div>
@if (ViewData["Count"] != null)
{

<div>The count is @ViewData["Count"]</div>

<ul>

    @for (int i = 0; i < (int)ViewData["Count"]; i++)
    {
        <li>Item @i</li>
    }
</ul>

} else
{
    <div>Count is not assigned</div>

}

